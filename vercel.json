{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/assets/(.*).js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/assets/(.*).css",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "http://173.249.7.231:8500/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [ { "type": "host", "value": "www.mailsfinder.com" } ],
      "destination": "https://mailsfinder.com/:path*",
      "permanent": true
    }
  ]
}